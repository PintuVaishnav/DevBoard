{
  "buildCommand": "npm run build",
  "outputDirectory": "client/dist",
  "framework": "vite",
 "rewrites": [
    { "source": "/api/(.*)", "destination": "https://localhost:5000/api/$1" },
    { "source": "/auth/(.*)", "destination": "https://localhost:5000/auth/$1" },
    { "source": "/(.*)", "destination": "/" }
  ]
}

